Option Explicit

Sub Test_CreationDocsWord()
    Dim ws As Worksheet
    Dim lastRow As Long, i As Long
    Dim url As String
    Dim wdApp As Object
    Dim wdDoc As Object
    
    Set ws = ActiveSheet
    lastRow = ws.Cells(ws.Rows.Count, "A").End(xlUp).Row
    
    ' Lancer Word
    On Error Resume Next
    Set wdApp = GetObject(, "Word.Application")
    On Error GoTo 0
    If wdApp Is Nothing Then
        Set wdApp = CreateObject("Word.Application")
    End If
    wdApp.Visible = True
    
    ' ðŸ”¹ BOUCLE SUR TOUTES LES LIGNES AVEC CONTENU EN COLONNE A
    For i = 1 To lastRow
        url = Trim$(ws.Cells(i, "A").Value)
        
        If url <> "" Then
            ' âžœ Ici on crÃ©e 1 document par URL, sans HTML, juste pour tester
            Set wdDoc = wdApp.Documents.Add
            
            With wdDoc.Range
                .Text = "Test document pour la ligne " & i & vbCrLf & _
                        "URL en colonne A : " & url
            End With
        End If
    Next i
    
    MsgBox "Test terminÃ©. Vous devriez avoir un document Word par URL non vide."
End Sub